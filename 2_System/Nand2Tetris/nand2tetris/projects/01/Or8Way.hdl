/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=in[0], b=in[1], out=fir1);
    Or(a=in[2], b=in[3], out=fir2);
    Or(a=in[4], b=in[5], out=fir3);
    Or(a=in[6], b=in[7], out=fir4);
    Or(a=fir1, b=fir2, out=sec1);
    Or(a=fir3, b=fir4, out=sec2);
    Or(a=sec1, b=sec2, out=out);
}
