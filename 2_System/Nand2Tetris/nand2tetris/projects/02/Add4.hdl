CHIP Add4 {
    IN a[4], b[4], cin;
    OUT out[4], carry;

    PARTS:
    FullAdder(a=a[0], b=b[0], c=cin, sum=out[0], carry=c0);
    FullAdder(a=a[1], b=b[1], c=c0, sum=out[1], carry=c1);
    FullAdder(a=a[2], b=b[2], c=c1, sum=out[2], carry=c2);
    FullAdder(a=a[3], b=b[3], c=c2, sum=out[3], carry=carry);
}
