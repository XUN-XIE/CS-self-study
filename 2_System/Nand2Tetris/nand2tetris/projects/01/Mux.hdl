/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

/**
(a & !sel) | (b & sel)
*/

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=notsel);
    And(a=a, b=notsel, out=first);
    And(a=b, b=sel, out=second);
    Or(a=first, b=second, out=out);
}
